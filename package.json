{
  "name": "wiseup-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --host 0.0.0.0",
    "dev": "vite --port 5173",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write .",
    "types": "tsc --noEmit",
    "generate:api": "openapi-generator-cli generate -i ../Backend/openapi.yaml -g typescript-fetch -o ./src/api/openapi-client --skip-validate-spec --additional-properties=inlineOneOf=false | grep -v 'WARN' | grep -v 'INFO'",
    "generate:api:backend": "cd ../Backend && npm run generate:api",
    "generate:api:all": "npm run generate:api && npm run generate:api:backend"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-hover-card": "1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@tailwindcss/vite": "4.1.8",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "eslint-plugin-react-dom": "1.50.0",
    "eslint-plugin-react-x": "1.50.0",
    "lucide-react": "0.513.0",
    "next-themes": "^0.4.6",
    "react": "18.3.1",
    "react-day-picker": "^9.8.0",
    "react-dom": "18.3.1",
    "react-easy-crop": "^5.4.2",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.57.0",
    "react-joyride": "^2.9.3",
    "react-router-dom": "7.6.2",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.5",
    "swr": "^2.3.3",
    "tailwind-merge": "3.3.0",
    "tailwindcss": "4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "9.28.0",
    "@openapitools/openapi-generator-cli": "2.20.2",
    "@types/node": "22.15.29",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "5.3.3",
    "@vitejs/plugin-react-swc": "3.10.0",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.2.0",
    "prettier": "3.5.3",
    "tw-animate-css": "1.3.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.0",
    "vite": "6.3.5"
  }
}
